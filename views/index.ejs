<% productChunks.forEach(function(eles) { %>
<div class="row">
  <% eles.forEach(function(ele) { %>
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <img src="<%= ele.imagePath %>" alt="..." class="img-responsive">
        <div class="caption">
          <h><%= ele.username %></h>
          <p><%= ele.description %></p>
          <p><a href="#" class="btn btn-primary" role="button">Button</a>
            <a href="#" class="btn btn-success" role="button">Good</a></p>
        </div>
      </div>
    </div>
  <% }) %>
</div>
<% }) %>
